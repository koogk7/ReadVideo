{
  // 필수
  "manifest_version": 2,// 고정
  "name": "안봐도 비디오 - 영상을 읽다",
  "version": "1.0.0",
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["./src/service/showSubtitle.js"]
  }],
  "content_security_policy": "script-src 'self' https://www.naver.com; object-src 'self'",
  "sidebar_action": {
    "default_page": "sidebar.html",  // 필수
    "default_icon": {                // 필수
      "16": "./image/logo_main.png"
    },
    "default_title": "사이드바앱",      // 선택
    "use_navigation_bar": false       // 선택
  },
  "icons": {
    "32": "./image/logo_main.png",
    "128": "./image/main_logo.png"
  },
  "permissions": [
    "<all_urls>",
    "activeTab"
  ]
}